package ereditarietà;

class Veicolo {
    void run(){
        System.out.println("Il veicolo è in marcia");
    }
}

class Bike extends Veicolo {
    public static void main(String[] args) {
        Bike bike = new Bike();
        bike.run();
    }
}

class Bike2 extends Veicolo {
    void run(){
        System.out.println("Bici in sicurezza");}
        public static void main(String[] args) { //NO 2 MAIN 
            Bike2 bike2 = new Bike2();
            bike2.run();
        }
}
